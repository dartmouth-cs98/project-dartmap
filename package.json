{
  "name": "marauders",
  "version": "1.0.0",
  "description": "a map of all events on campus",
  "main": "src/index.js",
  "scripts": {
    "start": "node_modules/.bin/webpack-dev-server --inline --hot",
    "test": "jest",
    "build": "node_modules/.bin/webpack"
  },
  "jest": {
    "verbose": "true",
    "collectCoverage": "true",
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/build/**",
      "!**/graveyard/**",
      "!**/images/**",
      "!**/milestones/**"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/node_modules/",
      "<rootDir>/graveyard/",
      "<rootDir>/milestones/",
      "<rootDir>/images/",
      "<rootDir>"
    ]
  },
  "author": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/dartmouth-cs98/project-dartmap.git"
  },
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-jest": "^16.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "css-loader": "^0.25.0",
    "enzyme": "^2.5.1",
    "eslint": "^3.7.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "jest": "^16.0.2",
    "node-sass": "^3.10.1",
    "react-addons-test-utils": "^15.3.2",
    "react-test-renderer": "^15.3.2",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "axios": "^0.15.0",
    "google-map-react": "^0.20.0",
    "jquery": "^3.1.1",
    "lodash.debounce": "^4.0.8",
    "moment": "^2.15.1",
    "rc-slider": "^5.1.2",
    "react": "^15.3.2",
    "react-controllables": "^0.6.0",
    "react-datetime": "^2.6.0",
    "react-dom": "^15.3.2",
    "react-router": "^2.8.1"
  }
